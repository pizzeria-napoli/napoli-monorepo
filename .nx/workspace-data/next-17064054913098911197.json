{
  "6800881335368006080": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "napoli-pwa",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/napoli-pwa/.next/!(cache)/**/*",
        "{workspaceRoot}/napoli-pwa/.next/!(cache)"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "napoli-pwa"
      }
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "napoli-pwa"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "napoli-pwa"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "pnpm exec nx watch --projects napoli-pwa --includeDependentProjects -- pnpm exec nx build-deps napoli-pwa"
    }
  }
}